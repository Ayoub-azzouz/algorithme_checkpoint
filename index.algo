ALGORITHM 

VAR
  i,w,v,c:INTEGER;
  ch=STRING;
BEGIN
FOR i FROM 0 TO ch.length - 1  STEP step  DO
    IF (ch[i] != " ") THEN
        ++c ;
    END_IF 
    END_FOR
    FOR i FROM 0 TO ch.length - 1  STEP step  DO
    let w=1 ;
    IF (ch[i] == " ") THEN
        ++w ;
    END_IF 
    END_FOR
    FOR i FROM 0 TO ch.length - 1  STEP step  DO
    IF (ch[i] == "a") or (ch[i] == "o") or (ch[i] == "u") or (ch[i] == "e") or (ch[i] == "i") THEN
        ++v ;
    END_IF 
    END_FOR



END